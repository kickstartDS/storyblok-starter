{
  "name": "@kickstartds/storyblok-starter",
  "version": "1.0.0",
  "license": "(MIT OR Apache-2.0)",
  "author": {
    "name": "Jonas Ulrich",
    "email": "jonas.ulrich@kickstartds.com"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "BUILD_ENV=preview next dev",
    "dev:proxy": "wait-on http://localhost:3000/ && local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "build": "npm run build-tokens && npm run extract-tokens && npm run blurhashes && npm run bundle-static-assets && next build",
    "postbuild": "next-sitemap",
    "export": "next build && next export",
    "build-preview": "BUILD_ENV=preview npm run build",
    "netrc": "plop netrc $STORYBLOK_LOGIN_EMAIL $NEXT_STORYBLOK_OAUTH_TOKEN $STORYBLOK_REGION",
    "env": "plop env $NEXT_STORYBLOK_API_TOKEN $NEXT_STORYBLOK_OAUTH_TOKEN $NEXT_STORYBLOK_SPACE_ID",
    "netlify-init": "npm run env && npm run netrc && npm run init",
    "blurhashes": "mkdir -p public/blurhashes && node scripts/createBlurHashes.js",
    "init": "rimraf tmp && mkdir -p tmp/images && node scripts/prepareProject.js && npm run push-components && npm run generate-content-types",
    "update-previews": "node scripts/updatePreviews.js",
    "dereference-schemas": "kickstartDS schema dereference --components-path node_modules/@kickstartds/ds-agency/dist/components --cms-path components",
    "generate-props": "kickstartDS schema types --components-path node_modules/@kickstartds/ds-agency/dist/components --cms-path components --merge-schemas false",
    "start": "next start",
    "lint": "next lint",
    "init-tokens": "kickstartDS tokens init --token-dictionary-path token/dictionary --branding-token-path token/branding-token.json",
    "build-tokens": "kickstartDS tokens compile --token-dictionary-path token/dictionary",
    "extract-tokens": "node scripts/extractComponentToken.js && node scripts/calculateCssProperties.js",
    "pull-content-schema": "dotenvx run -f .env.local -- npm run storyblok-pull-content-schema",
    "storyblok-pull-content-schema": "storyblok pull-components --space $NEXT_STORYBLOK_SPACE_ID -p types/ && mv types/components.$NEXT_STORYBLOK_SPACE_ID.json types/components-schema.json && mv types/presets.$NEXT_STORYBLOK_SPACE_ID.json types/components-presets.json",
    "generate-content-types": "dotenvx run -f .env.local -- npm run pull-content-schema && storyblok-generate-ts source=./types/components-schema.json target=./types/components-schema compilerOptions.unknownAny=true compilerOptions.additionalProperties=false",
    "push-components": "dotenvx run -f .env.local -- npm run storyblok-push-components",
    "storyblok-push-components": "storyblok push-components cms/components.123456.json --space $NEXT_STORYBLOK_SPACE_ID --presets-source cms/presets.123456.json",
    "create-storyblok-config": "kickstartDS cms storyblok --components-path node_modules/@kickstartds/ds-agency/dist/components --cms-path components --configuration-path cms --components blog-teaser contact cta divider faq features gallery hero html image-story image-text info-table logos mosaic slider stats teaser-card testimonials text video-curtain",
    "bundle-static-assets": "node scripts/bundleStaticAssets.js",
    "storyblok-logout": "storyblok logout",
    "storyblok-login": "storyblok login"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.7.0",
    "@kickstartds/base": "4.0.2",
    "@kickstartds/blog": "4.0.2",
    "@kickstartds/content": "4.1.1",
    "@kickstartds/core": "4.0.2",
    "@kickstartds/ds-agency": "^3.6.57",
    "@kickstartds/form": "4.0.2",
    "@storyblok/react": "^3.0.9",
    "@unpic/pixels": "^1.2.2",
    "@unpic/placeholder": "^0.1.2",
    "@unpic/react": "^0.1.14",
    "blurhash": "^2.0.5",
    "fast-glob": "^3.3.2",
    "file-system-cache": "^2.4.4",
    "lazysizes": "^5.3.2",
    "next-sitemap": "^4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "slugify": "^1.6.6",
    "storyblok-rich-text-react-renderer": "^2.9.0"
  },
  "devDependencies": {
    "@csstools/css-calc": "^1.2.4",
    "@csstools/css-parser-algorithms": "^2.7.1",
    "@csstools/css-tokenizer": "^2.4.1",
    "@kickstartds/jsonschema-utils": "^3.4.3",
    "@kickstartds/jsonschema2storyblok": "^1.6.2",
    "@kickstartds/jsonschema2types": "^1.1.54",
    "@kickstartds/style-dictionary": "^4.0.2",
    "@netlify/plugin-nextjs": "^5.5.1",
    "@types/ffprobe": "^1.1.8",
    "@types/ffprobe-static": "^2.0.3",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "custom-property-extract": "^1.2.1",
    "esbuild": "^0.19.5",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "fast-glob": "^3.3.2",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.1.0",
    "form-data": "^4.0.0",
    "image-size": "^1.1.1",
    "import-meta-resolve": "~3.0.0",
    "json-schema-typed": "~8.0.1",
    "jsonpointer": "^5.0.1",
    "kickstartds": "^3.2.0",
    "local-ssl-proxy": "^2.0.5",
    "next": "13.5.6",
    "npm-run-all": "^4.1.5",
    "object-traversal": "^1.0.1",
    "path-exists-cli": "^2.0.0",
    "plop": "^4.0.1",
    "promise-throttle": "^1.1.2",
    "rimraf": "^5.0.5",
    "storyblok": "^3.30.0",
    "storyblok-generate-ts": "^2.1.0",
    "typescript": "5.1.6",
    "wait-on": "^7.2.0"
  }
}
